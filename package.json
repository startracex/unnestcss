{
  "name": "unnestcss",
  "version": "1.1.0",
  "description": "A library for un-nesting CSS",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "lint": "biome lint --fix",
    "lint:check": "biome lint",
    "prepare": "lefthook install",
    "test": "vitest"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@startracex/dev-config": "^0.5.0",
    "@types/node": "^24.3.0",
    "dprint": "^0.50.1",
    "lefthook": "^1.12.3",
    "rollup": "4.48.1",
    "rollup-plugin-oxc": "^0.7.0",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": "./out/index.js",
      "require": "./out/index.cjs"
    },
    "./*.js": {
      "import": "./out/*.js",
      "require": "./out/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "out",
    "src"
  ],
  "license": "MIT",
  "author": "startracex",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/unnestcss.git"
  },
  "packageManager": "pnpm@10.15.0"
}
